TabTwo:AddToggle({
	Name = "Auto Equip Best",
	Default = false,
	Callback = function(Value)
	_G.autoBest = Value
    while _G.autoBest and wait() do
		
game:GetService("ReplicatedStorage").Events.EquipBest:FireServer()

	end    
	end
})


  _G.autoEgg = true
  _G.selEgg = "1"

    function autoEgg()
        while _G.autoEgg == true do
game:GetService("ReplicatedStorage").Events.PlayerPressedKeyOnEgg:FireServer(_G.selEgg,1)

            wait(0)
        end
    end

TabTwo:AddToggle({
Name = "Auto Hatch",
    Default = false,
    Callback = function(Value)
                _G.autoEgg = Value
                    autoEgg()
                    end
        })


